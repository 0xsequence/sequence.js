    "test:concurrently": "concurrently -k --success first 'pnpm start:hardhat2 > /dev/null'",
    "start:hardhat2": "hardhat node --hostname 0.0.0.0 --port 7147 --config ./hardhat2.config.js",
    "test:coverage": "nyc pnpm test"
  },
  "dependencies": {
    "@0xsequence/core": "^0.43.4",
    "@0xsequence/sessions": "^0.43.4",
